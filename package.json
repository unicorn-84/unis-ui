{
  "name": "@web-uni/unis",
  "description": "UI components for react.js websites and apps",
  "license": "MIT",
  "version": "0.1.2",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "author": "unicorn-84 <unicorn-84@ya.ru> (https://github.com/unicorn-84)",
  "title": "Uni UI",
  "module": "dist/unis.esm.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "@web-uni",
    "unis",
    "react",
    "components",
    "ui"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unicorn-84/unis.git"
  },
  "bugs": {
    "url": "https://github.com/unicorn-84/unis/issues"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "test:watch": "tsdx test --watchAll",
    "test:coverage": "tsdx test --coverage",
    "lint": "tsdx lint . --fix",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006 --ci",
    "build-storybook": "build-storybook",
    "preversion": "npm run lint && npm test",
    "postversion": "git push && git push --tags"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "tabWidth": 2
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@storybook/addon-essentials": "^6.1.15",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.1.15",
    "@storybook/addons": "^6.1.20",
    "@storybook/react": "^6.1.15",
    "@storybook/theming": "^6.1.20",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@types/jest-axe": "^3.5.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.7",
    "babel-loader": "^8.2.2",
    "chromatic": "^5.6.3",
    "husky": "^4.3.8",
    "jest-axe": "^4.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "styled-components": "^5.2.1"
  }
}
