{
  "name": "@web-uni/unis",
  "version": "0.0.0-development",
  "description": "UI components for react.js websites and apps",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14"
  },
  "keywords": [
    "react",
    "components",
    "ui"
  ],
  "author": "unicorn-84 <unicorn-84@ya.ru> (https://github.com/unicorn-84)",
  "repository": {
    "type": "git",
    "url": "https://github.com/unicorn-84/unis.git"
  },
  "bugs": {
    "url": "https://github.com/unicorn-84/unis/issues"
  },
  "scripts": {
    "semantic-release": "semantic-release",
    "cm": "cz",
    "format": "prettier -c .",
    "lint": "eslint .",
    "build": "rollup -c",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --ci --passWithNoTests",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "dependencies": {
    "react": "17.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "21.0.2",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@testing-library/jest-dom": "5.16.3",
    "@testing-library/react": "12.1.4",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.23",
    "@types/react": "17.0.42",
    "@types/react-dom": "17.0.14",
    "@typescript-eslint/eslint-plugin": "5.16.0",
    "@typescript-eslint/parser": "5.16.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.11.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-react": "7.29.4",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "prettier": "2.6.0",
    "react-dom": "17.0.2",
    "rollup": "2.70.1",
    "rollup-plugin-dts": "4.2.0",
    "rollup-plugin-typescript2": "0.31.2",
    "semantic-release": "^19.0.2",
    "ts-jest": "27.1.3",
    "tslib": "2.3.1",
    "typescript": "4.6.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  }
}
